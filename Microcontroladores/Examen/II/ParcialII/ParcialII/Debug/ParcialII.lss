000002 940c 00f1                 jmp EXT_INT0 ; IRQ0 Handler
000004 940c 00f2                 jmp EXT_INT1 ; IRQ1 Handler
000006 940c 00f3                 jmp TIM2_COMP ; Timer2 Compare Handler
000008 940c 00f4                 jmp TIM2_OVF ; Timer2 Overflow Handler
00000a 940c 00f5                 jmp TIM1_CAPT ; Timer1 Capture Handler
00000c 940c 00f6                 jmp TIM1_COMPA ; Timer1 CompareA Handler
00000e 940c 00f7                 jmp TIM1_COMPB ; Timer1 CompareB Handler
000010 940c 00f8                 jmp TIM1_OVF ; Timer1 Overflow Handler
000012 940c 00f9                 jmp TIM0_OVF ; Timer0 Overflow Handler
000014 940c 00fa                 jmp SPI_STC ; SPI Transfer Complete Handler
000016 940c 00fb                 jmp USART_RXC ; USART RX Complete Handler
000018 940c 00fc                 jmp USART_UDRE ; UDR Empty Handler
00001a 940c 00fd                 jmp USART_TXC ; USART TX Complete Handler
00001c 940c 00fe                 jmp ADC_COMP ; ADC Conversion Complete Handler
00001e 940c 00ff                 jmp EE_RDY ; EEPROM Ready Handler
000020 940c 0100                 jmp ANA_COMP ; Analog Comparator Handler
000022 940c 0101                 jmp TWSI ; Two-wire Serial Interface Handler
000024 940c 0102                 jmp EXT_INT2 ; IRQ2 Handler
000026 940c 0103                 jmp TIM0_COMP ; Timer0 Compare Handler
000028 940c 0104                 jmp SPM_RDY ; Store Program Memory Ready Handler
                                 
