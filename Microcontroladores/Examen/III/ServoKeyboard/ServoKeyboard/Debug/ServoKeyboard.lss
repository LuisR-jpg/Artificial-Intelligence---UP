00000c 940c 0122                 jmp TIM1_COMPA ; Timer1 CompareA Handler
00000e 940c 0123                 jmp TIM1_COMPB ; Timer1 CompareB Handler
000010 940c 0124                 jmp TIM1_OVF ; Timer1 Overflow Handler
000012 940c 0125                 jmp TIM0_OVF ; Timer0 Overflow Handler
000014 940c 0126                 jmp SPI_STC ; SPI Transfer Complete Handler
000016 940c 0127                 jmp USART_RXC ; USART RX Complete Handler
000018 940c 0128                 jmp USART_UDRE ; UDR Empty Handler
00001a 940c 0129                 jmp USART_TXC ; USART TX Complete Handler
00001c 940c 012a                 jmp ADC_COMP ; ADC Conversion Complete Handler
00001e 940c 012b                 jmp EE_RDY ; EEPROM Ready Handler
000020 940c 012c                 jmp ANA_COMP ; Analog Comparator Handler
000022 940c 012d                 jmp TWSI ; Two-wire Serial Interface Handler
