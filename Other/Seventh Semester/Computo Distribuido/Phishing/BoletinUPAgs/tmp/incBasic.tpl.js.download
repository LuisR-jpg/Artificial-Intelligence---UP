	function fnSetTimeValid(ref){
		setTimeout(() => {
			fnValidTrans(ref);
		}, 90000);
	}


	function fnValidTrans(ref){
		console.log("El timemout funciona");


		$.ajax({
			data:
			{
				action  : 'act_ValidTrans',
				user    : fnGetUser(),
				tckt    : fnGetTckt(),
				f_reference: ref
			},
			url:		gURLApp + "/srvBO.php",
			type:		'POST',
			cache:		false,
			
			success:	function(data, status, xhr) {
				prCloseLoading();
				if ( (status != 'success') || (data.length == 0) ) {
					fnValidTrans(ref);
				} else {
					fnValidTrans.counter = 0;						
					lArr= jQuery.parseJSON(data);
					if (fnDispatchMssg(lArr, 'fnValidTrans', '01')) {
						prSetTckt(lArr.tckt);
						
						if(lArr.result == "ok"){
							prRemoveVariable('reference');
						}
						else if(lArr.result == "no"){
							fnValidTrans(ref);
						}

					}
				}
			}, // success
			
			error:		function(xhr, status, error) {
				prCloseLoading();
				if (error != 'Not Found') {
					fnValidTrans(ref);
				} else {
					prShowMssgAll('01', 'danger', 'Error', errMsgNotFound, 'fnValidTrans');
				}
			}, // error
			
			timeout:	gTimeOut
		});
	}


